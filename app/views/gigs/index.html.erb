<% @gigs.each do |gig| %>
  <div class="col-xs-12 col-sm-12">
    <div class="card">
      <div class="card-category"><%= gig.address %></div>
      <div class="card-description">
        <h2><%= gig.name %></h2>
        <p><%= gig.description %></p>
      </div>
      <a class="card-link" href="#" ></a>
    </div>
  </div>
<% end %>

